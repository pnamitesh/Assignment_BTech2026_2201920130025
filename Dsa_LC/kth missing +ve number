class Solution {
    public int findKthPositive(int[] arr, int k) {
       
       int i=0,miss=0,num=1;
       while(true){
        if (i<arr.length&&arr[i]==num){
            i++;
        }

        else{
            miss++;
            if (miss==k) return num;
        } 
        num++;
       }
   
}
}
    
